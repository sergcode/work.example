mixin moscow-tabs
  #moscow.tab-pane.tariffs-political__tab-pane.fade.show.active(role="tabpanel" aria-labelledby="moscow-tab")
    .tariffs-political__carousel-wrapper
      .fotorama.fotorama__tariffs-political(data-auto="false")
        img(src=staticImg + "tariffs-political/mersedes-benz-e-class-w213-one.jpg" data-caption="Mercedes E W213")
        img(src=staticImg + "tariffs-political/mersedes-benz-e-class-w213-two.jpg" data-caption="Mercedes E W213")
        img(src=staticImg + "tariffs-political/mersedes-benz-e-class-w213-three.jpg" data-caption="Mercedes E W213")
        img(src=staticImg + "tariffs-political/mersedes-benz-e-class-w213-four.jpg" data-caption="Mercedes E W213")
        img(src=staticImg + "tariffs-political/mersedes-benz-e-class-w213-five.jpg" data-caption="Mercedes E W213")

    .tariffs-political__information
      #accordionTariffsPoliticalMoscow.tariffs-political__accordion

        .tariffs-political__card
          #moscowHeadingOne.tariffs-political__card-header
            h2
              button(type="button" data-toggle="collapse" data-target="#moscowCollapseOne" aria-expanded="true" aria-controls="moscowCollapseOne")
                | Mercedes-Benz S class W222 Maybach
          #moscowCollapseOne.collapse.show.tariffs-political__collapse(aria-labelledby="moscowHeadingOne" data-parent="#accordionTariffsPoliticalMoscow")
            p Доступно для заказа: 27 автомобилей
            p Год выпуска: 2019 г. | 2018 г. | 2017 г.
            ul
              li AMG пакет
              li ЖК панель
              li Панорамная крыша

        .tariffs-political__card
          #moscowHeadingTwo.tariffs-political__card-header
            h2
              button.collapsed(type="button" data-toggle="collapse" data-target="#moscowCollapseTwo" aria-expanded="false" aria-controls="moscowCollapseTwo")
                | Mercedes-Benz S class W222
          #moscowCollapseTwo.collapse.tariffs-political__collapse(aria-labelledby="moscowHeadingTwo" data-parent="#accordionTariffsPoliticalMoscow")
            p Доступно для заказа: 27 автомобилей
            p Год выпуска: 2019 г. | 2018 г. | 2017 г.
            ul
              li AMG пакет
              li ЖК панель
              li Панорамная крыша

        .tariffs-political__card
          #moscowHeadingThree.tariffs-political__card-header
            h2
              button.collapsed(type="button" data-toggle="collapse" data-target="#moscowCollapseThree" aria-expanded="false" aria-controls="moscowCollapseThree")
                | Mercedes-Benz E class W213
          #moscowCollapseThree.collapse.tariffs-political__collapse(aria-labelledby="moscowHeadingThree" data-parent="#accordionTariffsPoliticalMoscow")
            p Доступно для заказа: 27 автомобилей
            p Год выпуска: 2019 г. | 2018 г. | 2017 г.
            ul
              li AMG пакет
              li ЖК панель
              li Панорамная крыша


        .tariffs-political__card.tariffs-political__card_mb-40
          #moscowHeadingFour.tariffs-political__card-header
            h2
              button.collapsed(type="button" data-toggle="collapse" data-target="#moscowCollapseFour" aria-expanded="false" aria-controls="moscowCollapseFour")
                | Mercedes-Benz V class
          #moscowCollapseFour.collapse.tariffs-political__collapse(aria-labelledby="moscowHeadingFour" data-parent="#accordionTariffsPoliticalMoscow")
            p Доступно для заказа: 27 автомобилей
            p Год выпуска: 2019 г. | 2018 г. | 2017 г.
            ul
              li AMG пакет
              li ЖК панель
              li Панорамная крыша

      +button-middle-gold-bg('button__animation-glare button__animation-glare_delay-1', 'button', 'Заказать автомобиль', 'button', 'modal', '#modalConsultation')

